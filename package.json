{
  "name": "king_stock_backend",
  "version": "1.0.0",
  "main": "init.js",
  "repository": "https://github.com/wjdrudgh123/king_stock_backend.git",
  "author": "Jung-Jaeho <wjdrudgh123@naver.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/init.js",
    "build": "npm run build:server",
    "prebuild": "rm -rf build",
    "build:server": "babel src --out-dir build && cp -r src/views build/views && cp -r src/static/css build/static/css",
    "start": "pm2 start build/init.js --name 'stock' --log-date-format 'YYYY-MM-DD HH:mm'",
    "prestart": "npm run build",
    "pro:status": "pm2 list",
    "pro:stop": "pm2 stop stock",
    "pro:logs": "pm2 logs stock"
  },
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "mongoose": "^5.11.4",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.6",
    "pm2": "^4.5.0",
    "pug": "^3.0.0",
    "selenium-webdriver": "^4.0.0-alpha.8"
  }
}
